<div id="navigation-viewlet-container"
    tal:define="isCompilation viewlet/isCompilation;
                isContentReference viewlet/isContentReference;
                currentItem nocall:context;">
    <tal:compliationroot condition="isCompilation">
    <span><a tal:attributes="href viewlet/getStartURL" href="#">Start</a></span>
    </tal:compliationroot>
    <tal:contentreference condition="isContentReference">
    <span><a tal:attributes="href python:viewlet.getNextURL(currentItem)" href="#">Next</a></span>|
    <span><a tal:attributes="href python:viewlet.getPreviousURL(currentItem)" href="#">Previous</a></span>
    </tal:contentreference>
</div>
